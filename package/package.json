{
  "name": "@nuxtjs/salesforce",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "repository": "nuxt-community/salesforce-module",
  "description": "Salesforce integration for Nuxt",
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module",
    "salesforce",
    "crm"
  ],
  "author": {
    "name": "Jace Bennest <jace.bennest@safe.com>",
    "url": "https://github.com/safejace"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "build": "nuxt-module-build",
    "lint": "eslint --fix --ext .js,.ts,.vue .",
    "test": "yarn lint && yarn test:unit",
    "test:unit": "vitest run test/unit/*.test.*"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0-rc.11",
    "@salesforce/core": "^2.37.1",
    "chalk": "^5.0.1",
    "defu": "^6.1.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/events": "^3.0.0",
    "@types/node": "^18.6.2",
    "eslint": "latest",
    "nuxi": "^3.0.0-rc.11",
    "nuxt": "^3.0.0-rc.11",
    "typescript": "^4.8.4",
    "vitest": "^0.23.4",
    "vue": "^3.2.40"
  }
}
